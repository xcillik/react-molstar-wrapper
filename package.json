{
  "name": "react-molstar-wrapper",
  "private": false,
  "type": "module",
  "keywords": [
    "react",
    "molstar",
    "typescript"
  ],
  "description": "A React wrapper for Mol* (Molstar) molecular visualization library.",
  "author": {
    "name": "react-molstar-wrapper maintainers"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xcillik/react-molstar-wrapper"
  },
  "bugs": {
    "url": "https://github.com/xcillik/react-molstar-wrapper/issues"
  },
  "homepage": "https://github.com/xcillik/react-molstar-wrapper",
  "maintainers": [
    "Jakub Čillík <jakub@cillik.org>",
    "Romana Ďuráčiová <duraciova@mail.muni.cz>",
    "Michal Mikuš"
  ],
  "version": "0.0.3",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type:check": "tsc --noEmit",
    "tsc": "tsc --noEmit --watch",
    "check": "bun run type:check && bun run lint && bun run format:check"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./style.css": "./dist/react-molstar-wrapper.css"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/bun": "latest",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "typescript": "^5.9.3",
    "prettier": "3.6.2",
    "sass-embedded": "^1.93.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "19.2.0-canary-0bdb9206-20250818",
    "react-dom": "19.2.0-canary-0bdb9206-20250818"
  },
  "dependencies": {
    "molstar": "^5.4.1",
    "@tailwindcss/vite": "^4.1.17",
    "tailwindcss": "^4.1.17"
  }
}
